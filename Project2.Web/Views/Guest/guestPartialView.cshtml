@model List<Project2.Core.Models.Entities.Guest>
<thead>
    <tr>
        <th>#</th>
        <th>Tài khoản</th>
        <th>Mật khẩu</th>
        <th>Họ và tên</th>
        <th>Ảnh</th>
        <th>Ngày sinh</th>
        <th>Email</th>
        <th>Action</th>
    </tr>
</thead>
<tbody>
    @{
        int index = 0;
    }
    @foreach (var item in Model)
    {
        index++;
    <tr>
        <td>@index</td>
        <td>@item.Username</td>
        <td>@item.Password</td>
        <td>@item.Full_name</td>
        <td>@item.Path</td>
        <td>@item.Birthday</td>
        <td>@item.Email</td>
        <td>
            <a href="@Url.Action("Edit", "Guest", new { @id = item.Id })" class="btn btn-info btn-sm"><i class="fas fa-pen"></i></a>
            <a href="@Url.Action("Remove", "Guest", new { @id = item.Id })" class="btn btn-danger btn-sm"><i class="fas fa-trash"></i></a>
        </td>
    </tr>

    }
</tbody>

